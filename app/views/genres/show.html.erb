<h1><%= @genre.name %></h1>
<h2>Artists:</h2>
<ul>
  <% @genre.artists.each do |artist| %>
  <li>
    <a href="artists/<%= artist.to_slug %>.html">
      <%= artist.name %>
    </a>
  </li>
  <% end %>
</ul>

   <% me = Genre.find_by(name: self) %>
  <%= #{me.name} %>
  <%= #{me.artists} %>
  <%= #{me.songs} %>

